

Table artists {
artist_id varchar [pk]
name varchar 
location varchar 
latitude numeric 
longitude numeric 
}

Table songplays {
songplay_id int4 [pk]
start_time timestamp 
user_id int4 
level varchar 
song_id varchar 
artist_id varchar 
session_id int4 
location varchar 
user_agent varchar 
}

Table songs {
song_id varchar [pk]
title varchar 
artist_id varchar 
year int4 
duration numeric 
}

Table time {
start_time timestamp 
hour int4 
day int4 
week int4 
month int4 
year int4 
weekday int4 
}

Table users {
user_id int4 [pk]
first_name varchar 
last_name varchar 
gender varchar 
level varchar 
}

Ref: artists.artist_id < songplays.artist_id
Ref: artists.artist_id < songs.artist_id
Ref: songs.song_id < songplays.song_id
Ref: users.user_id < songplays.user_id
